<div class="container-fluid">
  <div class="row col-12">
    <h4>{{title}}</h4>

    <table class="table table-bordered">
      <thead>
        <tr>

          <th>User Name</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Role Id</th>
          <th>Phone</th>
        </tr>
      </thead>
      <tbody id="tbyburgerDetails">
        {{#each users}}
        <tr id="{{this.id}}">

          <td>{{this.username}}</td>
          <td>{{this.firstname}}</td>
          <td>{{this.lastname}}</td>
          <td>{{this.roleid}}</td>
          <td>{{this.phone}}</td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>